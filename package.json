{
  "name": "sk-components",
  "version": "3.1.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "csscomb packages && csscomb apps && prettier --write \"**/*.{scss,ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "csscomb": "^3.1.8",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "react": "^18.2.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.3"
}
